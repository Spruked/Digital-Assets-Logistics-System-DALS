{
  "dashboard": {
    "id": null,
    "title": "DALS Phase 11-A2 - Autonomous Predictive Prevention",
    "tags": ["dals", "caleon", "predictive", "autonomous"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Health Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"dals-controller\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "HEALTHY",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Predictive Engine Status",
        "type": "stat",
        "targets": [
          {
            "expr": "predictive_engine_active",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "INACTIVE",
                    "color": "orange"
                  },
                  "1": {
                    "text": "ACTIVE",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Risk Score Trend",
        "type": "graph",
        "targets": [
          {
            "expr": "predictive_risk_score",
            "refId": "A",
            "legendFormat": "Risk Score"
          }
        ],
        "yAxes": [
          {
            "format": "percent",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Prevention Actions Executed",
        "type": "table",
        "targets": [
          {
            "expr": "predictive_prevention_actions_total",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "CANS Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "cans_response_time_seconds",
            "refId": "A",
            "legendFormat": "Response Time"
          }
        ],
        "yAxes": [
          {
            "format": "s",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Self-Model Confidence",
        "type": "gauge",
        "targets": [
          {
            "expr": "self_model_confidence_percent",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "thresholds": [
              {
                "value": 70,
                "color": "green"
              },
              {
                "value": 85,
                "color": "yellow"
              },
              {
                "value": 95,
                "color": "red"
              }
            ]
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 7,
        "title": "Voice Awareness Activity",
        "type": "stat",
        "targets": [
          {
            "expr": "voice_awareness_responses_total",
            "refId": "A"
          }
